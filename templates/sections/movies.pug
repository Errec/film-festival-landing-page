section.movies.container
  ul.movies__carousel.container-fluid
    -for (var index = 1; index < 15; ++index)
      li(class="card")
        img(class="card-img-top" src="/img/cover_" + index + ".jpg" alt="Card image cap")
        ul(class="list-group list-group-flush")
          li(class="list-group-item") Day: August 5th
          li(class="list-group-item") Time: 10:00pm
